<%= form_for(@list) do |f| %>
	<div class="field">
	  <%= f.label :title, class: "control-label" %>
	  <div class="input-group">
		<%= f.text_field :title, class: "form-control" %>
		<span class="input-group-btn">
			<%= f.submit "Submit!", class: "btn btn-primary" %>
      </span>
	  </div>
	</div>
<% end %>
